{
  "name": "server",
  "version": "1.0.0",
  "description": "WeConnect - Business connect app",
  "main": "app.js",
  "engines": {
    "node": "8.9.4",
    "npm": "5.7.x"
  },
  "scripts": {
    "test": "mocha test --exit",
    "clean": "rm -rf dist",
    "postinstall": " npm run clean && mkdir dist && babel ./ -s -d dist --ignore node_modules",
